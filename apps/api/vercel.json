{
  "version": 2,
  "installCommand": "cd ../../ && pnpm install",
  "buildCommand": "cd ../../ && turbo run build --filter=@fina/api",
  "outputDirectory": "dist",
  "devCommand": "cd ../../ && pnpm dev --filter=@fina/api",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/apps/api/src/main.ts" }]
}
